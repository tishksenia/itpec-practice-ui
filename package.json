    "autoprefixer": "^10.4.21",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
